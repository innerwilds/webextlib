{
  "name": "tsr-auto-library",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.cjs.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "build": "run-s build:clean build:pro build:types build:copy",
    "build:clean": "if exist lib rmdir /S/ Q lib && if exist types rmdir /S /Q types",
    "build:copy": "copyfiles -u 1 src/*.d.ts types",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:pro": "rollup -c --environment NODE_ENV:production",
    "build:types": "tsc -p tsconfig.d.json",
    "build:watch": "pnpm run build:dev -- -w",
    "prepublishOnly": "pnpm run build",
    "postpublish": "if exist node_modules rmdir /S /Q node_modules && if exist package-lock.json rmdir /S /Q package-lock.json && pnpm i"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/webextension-polyfill": "^0.9.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.78.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "deflib": "^1.0.1"
  }
}
